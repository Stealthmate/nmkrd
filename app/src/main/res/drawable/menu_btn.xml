<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <solid android:color="#00000000"/>
            <size
                android:width="150dp"
                android:height="150dp"
                />
        </shape>
    </item>

    <item
        android:left="30dp"
        android:right="30dp"
        android:top="40dp"
        android:bottom="100dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/nm_colorMenuBtn" />
            <corners
                android:radius="100dp"
                />
        </shape>
    </item>

    <item
        android:left="30dp"
        android:right="30dp"
        android:top="70dp"
        android:bottom="70dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/nm_colorMenuBtn" />

            <corners
                android:radius="100dp"
                />
        </shape>
    </item>

    <item
        android:left="30dp"
        android:right="30dp"
        android:top="100dp"
        android:bottom="40dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/nm_colorMenuBtn" />
            <corners
                android:radius="100dp"
                />
        </shape>
    </item>
</layer-list>
